def plus(a, b):
  return(int(a) + int(b))

def minus(a, b):
  return(int(a) - int(b))

def times(a, b):
  return(int(a) * int(b))

def division(a, b):
  return(int(a) / int(b))

def negation(a):
  return(-int(a))

def power(a, b):
  return(int(a) ** int(b))

print(plus(99, 1))
print(minus(100, "1"))
print(times(2, 40))
print(division(1, "3"))
print(negation(100, 10))
print(power(2, 10))